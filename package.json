{
  "name": "hexproperty",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "docs:validate": "ts-node tools/documentation/validators/validate.ts",
    "docs:generate": "npm run docs:typedoc && npm run docs:api && npm run docs:diagrams",
    "docs:test": "jest --testMatch='**/docs/**/*.test.ts'",
    "docs:typedoc": "typedoc",
    "docs:api": "swagger-jsdoc -d tools/documentation/config/swagger.json -o docs/api/swagger.json",
    "docs:diagrams": "mmdc -i docs/diagrams/*.mmd -o docs/diagrams/generated/",
    "docs:dev": "docusaurus start",
    "docs:build": "docusaurus build",
    "docs:deploy": "docusaurus deploy",
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@tanstack/react-query": "^5.17.9",
    "chart.js": "^4.4.1",
    "framer-motion": "^10.18.0",
    "lucide-react": "^0.309.0",
    "next": "14.0.4",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@docusaurus/core": "^3.0.0",
    "@docusaurus/preset-classic": "^3.0.0",
    "@mermaid-js/mermaid-cli": "^10.6.1",
    "@swc/core": "^1.3.107",
    "@swc/jest": "^0.2.31",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.1.6",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "autoprefixer": "^10.4.16",
    "broken-link-checker": "^0.7.8",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "gh-pages": "^6.1.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.33",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tailwindcss": "^3.4.1",
    "typedoc": "^0.25.7",
    "typescript": "^5.3.3"
  }
}
